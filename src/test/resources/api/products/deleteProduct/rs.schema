{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer"
    },
    "title": {
      "type": "string"
    },
    "price": {
      "type": "integer"
    },
    "discountPercentage": {
      "type": "number"
    },
    "stock": {
      "type": "integer"
    },
    "rating": {
      "type": "number"
    },
    "images": {
      "type": "array",
      "items":
        {
          "type": "string"
        },
        "minItems": 0,
        "uniqueItems": true
    },
    "description": {
      "type": "string"
    },
    "brand": {
      "type": "string"
    },
    "category": {
      "type": "string"
    }
  },
  "required": [
    "id",
    "title",
    "price",
    "stock",
    "rating",
    "description",
    "brand",
    "category"
  ]
}
